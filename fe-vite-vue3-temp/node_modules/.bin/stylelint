#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/stylelint@16.0.2_typescript@5.5.3/node_modules/stylelint/bin/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/stylelint@16.0.2_typescript@5.5.3/node_modules/stylelint/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/stylelint@16.0.2_typescript@5.5.3/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/stylelint@16.0.2_typescript@5.5.3/node_modules/stylelint/bin/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/stylelint@16.0.2_typescript@5.5.3/node_modules/stylelint/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/stylelint@16.0.2_typescript@5.5.3/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../stylelint/bin/stylelint.mjs" "$@"
else
  exec node  "$basedir/../stylelint/bin/stylelint.mjs" "$@"
fi
