#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/vite@5.3.2_@types+node@20.14.9_sass@1.77.6_sugarss@2.0.0/node_modules/vite/bin/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/vite@5.3.2_@types+node@20.14.9_sass@1.77.6_sugarss@2.0.0/node_modules/vite/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/vite@5.3.2_@types+node@20.14.9_sass@1.77.6_sugarss@2.0.0/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/vite@5.3.2_@types+node@20.14.9_sass@1.77.6_sugarss@2.0.0/node_modules/vite/bin/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/vite@5.3.2_@types+node@20.14.9_sass@1.77.6_sugarss@2.0.0/node_modules/vite/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/vite@5.3.2_@types+node@20.14.9_sass@1.77.6_sugarss@2.0.0/node_modules:/mnt/e/VDhub/template-for-dev/fe-vite-vue3-temp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../vite@5.3.2_@types+node@20.14.9_sass@1.77.6_sugarss@2.0.0/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../../vite@5.3.2_@types+node@20.14.9_sass@1.77.6_sugarss@2.0.0/node_modules/vite/bin/vite.js" "$@"
fi
